{
  "name": "portfolio",
  "author": "Nicolas <highfivenico@gmail.com>",
  "description": "Monorepo contenant le portfolio front-end et l'API backend.",
  "private": true,
  "workspaces": [
    "apps/client",
    "apps/server"
  ],
  "scripts": {
    "dev:client": "npm --workspace apps/client run dev",
    "dev:server": "npm --workspace apps/server run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
